        <Param name="Begin Experiment" updates="constant" val="# Trial timings&amp;#10;t_rest = 15&amp;#10;t_ready = 2&amp;#10;t_tapping = 15&amp;#10;&amp;#10;# Start of tapping within trial&amp;#10;t_tapstart = t_rest + t_ready&amp;#10;&amp;#10;# Total trail duration&amp;#10;t_trial = t_tapstart + t_tapping&amp;#10;&amp;#10;# Tap interval&amp;#10;t_tapint = 0.2" valType="extendedCode"/>
